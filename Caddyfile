maas.missiontools.app:443 {
handle /generate_204 {
		redir https://my.missiontools.app/payment=true 302

	}
handle {

forward_proxy {

	ports     80 443
	disable_insecure_upstreams_check
	serve_pac /proxy.pac

	dial_timeout            30

	max_idle_conns          50
	max_idle_conns_per_host 2

	acl {
		 allow all
	}
}

}
}
